<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.examprep.entities.QuestionBank" table="QuestionBank">
        <id name="qBankID" type="long" column="qbankid" access="field">
            <generator class="native"/>
        </id>
        <property name="qBankName" type="string" column="qbankname" not-null="true"/>
        <property name="qBankCount" type="string" column="qbankcount" not-null="true"/>     
    </class>
</hibernate-mapping>