<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.examprep.entities.QuestionBank" table="ep_questionBank">
		<id name="qBankID" type="long" column="qbank_id" access="field">
			<generator class="native" />
		</id>
		<property name="qBankName" type="string" column="qbank_name"
			not-null="true" />
		<property name="qBankCount" type="string" column="qbank_count"
			not-null="true" />
	</class>
</hibernate-mapping>